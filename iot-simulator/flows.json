    {
        "id": "1b1f1aa2d8132244",
        "type": "function",
        "z": "ed92b1b71e656011",
        "name": " Sart Sensor",
        "func": "// Inicialização do miniDB \nlet info_recebidas = msg.req ? msg.req.body : (msg.payload || {});\nlet db_local = global.get(\"data_base_local\") || {};\n\nconst id = info_recebidas.id_sensor || info_recebidas.idSensor;\nif (!id) {\n    node.error(\"Payload de iniciar_monitoramento sem id_sensor\");\n    return msg;\n}\n\n//converter pesos para Number\nfunction toNum(v) {\n    if (v === undefined || v === null) return undefined;\n    const n = Number(v);\n    return isNaN(n) ? undefined : n;\n}\n\nconst pesoBrutoRaw = info_recebidas.peso_bruto ?? info_recebidas.pesoBruto;\nconst pesoTaraRaw  = info_recebidas.peso_tara  ?? info_recebidas.pesoTara;\nconst pesoFluidoRaw= info_recebidas.peso_fluido ?? info_recebidas.pesoFluido;\n\nconst pesoBruto = toNum(pesoBrutoRaw);\nconst pesoTara  = toNum(pesoTaraRaw);\nconst pesoFluido= toNum(pesoFluidoRaw);\n\nif (pesoBruto === undefined) {\n    node.warn(`Sensor ${id}: pesoBruto ausente ou inválido (${pesoBrutoRaw}). Armazenando como null.`);\n}\n\nif (pesoTara === undefined) {\n    node.warn(`Sensor ${id}: pesoTara ausente ou inválido (${pesoTaraRaw}).`);\n}\n\ndb_local[id] = {\n    id_sensor: id,\n    id_cliente: info_recebidas.id_cliente || info_recebidas.idCliente,\n    pesoBruto: pesoBruto === undefined ? null : pesoBruto,\n    pesoTara:  pesoTara  === undefined ? null : pesoTara,\n    pesoFluido:pesoFluido=== undefined ? null : pesoFluido,\n   \n    _lastUpdateAt: new Date().toISOString()\n};\n\nglobal.set(\"data_base_local\", db_local);\nmsg.miniDB = db_local[id];\nreturn msg;",

    {
        "id": "fd3fba57a84c4c74",
        "type": "function",
        "z": "ed92b1b71e656011",
        "name": "Leitura_status",
        "func": "\ntry {\n    // HTTP Request vem em msg.payload\n    const resp = msg.payload || {};\n    // Backend devolve { status: \"Renovado\" } ou { status: \"Em espera\" }\n    const status_recebido = resp.status || resp.status_gas || resp.message || \"\";\n\n    if (!status_recebido) {\n        throw new Error(\"Resposta de status vazia\");\n    }\n\n    const s = String(status_recebido).toLowerCase();\n\n    if (s === \"renovado\" || s === \"botijão renovado com sucesso!\" || s === \"renovado\") {\n        msg.triagens2 = \"1\";\n    } else if (s === \"em espera\" || s === \"análise\" || s === \"analise\") {\n        msg.triagens2 = \"2\";\n    } else if (s === \"cancelado\") {\n        msg.triagens2 = \"3\";\n    } else {\n        throw new Error(\"Valor de status inválido: \" + status_recebido);\n    }\n\n    return msg;\n} catch (error) {\n    node.error(\"Erro ao processar status: \" + error.message);\n    msg.payload = { error: \"Status não reconhecido\", details: error.message };\n    return msg;\n}",
